    "axios": "1.7.9",
    "moment": "2.30.1",
    "nanoid": "5.1.2"
  },
  "devDependencies": {
    "@types/ffmpeg-static": "3.0.3",
    "@types/fluent-ffmpeg": "2.1.27",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.4",
    "@types/node-cron": "3.0.11",
    "eslint": "9.20.1",
    "jest": "29.7.0",
    "jest-presets": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "5.7.3"
